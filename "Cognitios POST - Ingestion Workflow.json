
  "name": "Cognitios POST - Ingestion Workflow",
  "nodes": [
    {
      "parameters": {
        "public": true,
        "initialMessages": "Olá! Serei seu assistente em um incidente. Registre os eventos aqui para que eu possa organizar a timeline.",
        "options": {
          "allowFileUploads": true,
          "inputPlaceholder": "Descreva o que ocorreu...",
          "showWelcomeScreen": true,
          "getStarted": "New Conversation",
          "subtitle": "Utilize este canal para registrar todos os eventos do incidente em tempo real.",
          "title": "REGISTRO DE INCIDENTE",
          "customCss": "/* Tema Dark/Teal Customizado */"
        }
      },
      "type": "@n8n/n8n-nodes-langchain.chatTrigger",
      "typeVersion": 1.3,
      "position": [-368, -32],
      "id": "node_chat_trigger",
      "name": "When chat message received",
      "webhookId": "WEBHOOK_ID_TEMPLATE"
    },
    {
      "parameters": {
        "promptType": "define",
        "text": "={{ $('When chat message received').item.json.chatInput }}",
        "options": {
          "systemMessage": "Você é um robô de extração de dados JSON especializado em SRE. Sua tarefa é converter o relato do usuário em um objeto JSON com: 'tipo' (Alerta, Ação, Evidência, Decisão), 'conteudo' (reformulação profissional) e 'responsavel' (identificado ou 'Não especificado').\n\nEXEMPLO:\nEntrada: 'Analista A iniciou a investigação dos logs.'\nSaída: {\"tipo\": \"Ação\", \"conteudo\": \"Início da investigação de logs do sistema.\", \"responsavel\": \"Analista A\"}"
        }
      },
      "type": "@n8n/n8n-nodes-langchain.agent",
      "typeVersion": 3,
      "position": [48, -32],
      "id": "node_ai_agent",
      "name": "AI Agent"
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "set_msg",
              "name": "chat_mensagem",
              "value": "={{ $json.chatInput }}",
              "type": "string"
            },
            {
              "id": "set_time",
              "name": "chat_hora-mensagem",
              "value": "={{ $now.format('dd/MM/yyyy HH:mm:ss') }}",
              "type": "string"
            }
          ]
        }
      },
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.4,
      "position": [-144, -32],
      "id": "node_edit_fields",
      "name": "Edit Fields"
    },
    {
      "parameters": {
        "jsCode": "// Extração de segurança do JSON da IA\nconst aiOutput = $('AI Agent').item.json.output;\nconst match = aiOutput.match(/{[\\s\\S]*}/);\nif (match && match[0]) {\n  try {\n    return JSON.parse(match[0]);\n  } catch (e) {\n    return { error: \"Erro no parse\", raw: match[0] };\n  }\n}\nreturn { error: \"JSON não encontrado\" };"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [384, -32],
      "id": "node_code_js",
      "name": "Code in JavaScript"
    },
    {
      "parameters": {
        "operation": "append",
        "documentId": {
          "__rl": true,
          "mode": "id",
          "value": "ID_DA_SUA_PLANILHA_AQUI"
        },
        "sheetName": {
          "__rl": true,
          "value": "gid=0",
          "mode": "list"
        },
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "Hora da Mensagem": "={{ $('Edit Fields').item.json['chat_hora-mensagem'] }}",
            "Tipo Mensagem": "={{ $json.tipo }}",
            "Conteudo da Mensagem": "={{ $json.conteudo }}",
            "Responsável": "={{ $json.responsavel }}"
          }
        }
      },
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 4.7,
      "position": [608, -32],
      "id": "node_gsheets",
      "name": "Append row in sheet",
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "ID_SUA_CREDENCIAL_GOOGLE",
          "name": "Sua Conta Google"
        }
      }
    },
    {
      "parameters": {
        "message": "Evento registrado na timeline!",
        "waitUserReply": false
      },
      "type": "@n8n/n8n-nodes-langchain.chat",
      "typeVersion": 1,
      "position": [832, -32],
      "id": "node_respond_chat",
      "name": "Respond to Chat"
    },
    {
      "parameters": {
        "options": {}
      },
      "type": "@n8n/n8n-nodes-langchain.lmChatGoogleGemini",
      "typeVersion": 1,
      "position": [48, 96],
      "id": "node_gemini_model",
      "name": "Google Gemini",
      "credentials": {
        "googlePalmApi": {
          "id": "ID_SUA_CREDENCIAL_GEMINI",
          "name": "Sua Conta Gemini"
        }
      }
    }
  ],
  "connections": {
    "When chat message received": { "main": [[{ "node": "Edit Fields", "type": "main", "index": 0 }]] },
    "AI Agent": { "main": [[{ "node": "Code in JavaScript", "type": "main", "index": 0 }]] },
    "Edit Fields": { "main": [[{ "node": "AI Agent", "type": "main", "index": 0 }]] },
    "Code in JavaScript": { "main": [[{ "node": "Append row in sheet", "type": "main", "index": 0 }]] },
    "Append row in sheet": { "main": [[{ "node": "Respond to Chat", "type": "main", "index": 0 }]] },
    "Google Gemini": { "ai_languageModel": [[{ "node": "AI Agent", "type": "ai_languageModel", "index": 0 }]] }
  },
  "settings": { "executionOrder": "v1" }
}
